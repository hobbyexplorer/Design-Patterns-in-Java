<?xml version="1.0" encoding="UTF-8"?>
<persistence
    version="1.0"
    xmlns="http://java.sun.com/xml/ns/persistence"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="gamemaker" transaction-type="RESOURCE_LOCAL">
    <mapping-file>META-INF/orm.xml</mapping-file>
    <class>org.knownspace.gamemaker.server.entity.Sample</class>
    <class>org.knownspace.gamemaker.server.entity.SampleItem</class>
    <class>org.knownspace.gamemaker.server.entity.GameUser</class>
    <class>org.knownspace.gamemaker.server.entity.GameVersion</class>
     <class>org.knownspace.gamemaker.server.entity.Game</class>
     <class>org.knownspace.gamemaker.server.entity.GameKey</class>
     <class>org.knownspace.gamemaker.server.entity.GamePlusUser</class>
     <class>org.knownspace.gamemaker.server.entity.GameScore</class>
     <class>org.knownspace.gamemaker.server.entity.GameRating</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
  </persistence-unit>
</persistence>
